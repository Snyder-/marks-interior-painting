<div class="row">
  <div class="col-sm-2">
  </div>
  <div class="col-sm-8">
    <h2>Add a Photo</h2>
    <%= simple_form_for @photo, html: { class: 'text-left jumbotron' } do |f| %>
      <%= f.input :title, placeholder: "Fresh Paint!" %>
      <%= f.input :description, placeholder: "Love the smell of fresh new paint on the walls!" %>
      <%= f.input :image %>
      <%= f.button :submit, class: 'btn btn-success' %>
      <p></p>
      <%= link_to "Cancel", photos_path, class: 'btn btn-danger' %>
    <% end %>
  </div>
  <div class="col-sm-2">
  </div>
</div>
<p></p>
