<div class="center">
  <p>
  <%= image_tag @photo.image.url(:thumb) %>
  </p>

  <p class='jumbotron'>
  <%= @photo.name %>
  <%= @photo.description %>
  </p>

  <p>
  <%= link_to "Edit Photo", edit_photo_path, class: "btn btn-primary" %>
  <%= link_to "Delete Photo", @photo, method: :delete, data: { confirm: "Are you sure" }, class: 'btn btn-primary' %>
  </p>
</div>
